{
  "name": "vue-query-builder-vuetifyjs",
  "version": "0.7.1",
  "description": "Vue Query Builder is a user interface that makes it easy for your users to create queries of any kind. It's useful if you need an interface for generating reports, filtering data, and more. This fork move it to Vuetify.js",
  "main": "./dist/VueQueryBuilder.common.js",
  "scripts": {
    "serve": "vue-cli-service serve --name VueQueryBuilder src/main.js",
    "build": "vue-cli-service build --target lib --name VueQueryBuilder src/VueQueryBuilder.vue"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vuetify": "^2.1.15"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "babel-core": "7.0.0-bridge.0",
    "deepmerge": "^4.2.2",
    "fibers": "^4.0.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.0",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ]
}
